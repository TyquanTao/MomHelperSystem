package com.cn.pojo.vo;

import com.cn.pojo.Circle;

public class CircleVo extends Circle {

    // 判断用户是否关注 0未关注 1关注
    private Integer isFollow;
    // 统计关注数量
    private Integer countFollow;

    public Integer getIsFollow() {
        return isFollow;
    }

    public void setIsFollow(Integer isFollow) {
        this.isFollow = isFollow;
    }

    public Integer getCountFollow() {
        return countFollow;
    }

    public void setCountFollow(Integer countFollow) {
        this.countFollow = countFollow;
    }
}
